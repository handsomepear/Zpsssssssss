.withdraw-success-wrap {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.8);
}
.withdraw-success-modal {
  position: absolute;
  display: flex;
  flex-direction: column;
  align-items: center;
  top: 50%;
  left: 50%;
  width: 622rpx;
  height: 400rpx;
  transform: translate(-50%, -50%);
  background-color: #fff;
  border-radius: 10rpx;
  font-size: 40rpx;
  color: #000;
  padding: 30rpx 40rpx 0;
  box-sizing: border-box;
}
.withdraw-tips {
  font-size: 28rpx;
  color: #5c5c5c;
  margin-top: 35rpx;
  text-indent: 2em;
}
.withdraw-success-modal .confirm-btn {
  width: 210rpx;
  height: 76rpx;
  background-color: #ff6c00;
  color: #fff;
  font-size: 32rpx;
  text-align: center;
  line-height: 76rpx;
  border-radius: 76rpx;
  margin-top: 50rpx;
}